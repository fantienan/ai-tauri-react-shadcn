{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "ai-tauri-react-shadcn",
  "version": "0.1.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "../client/dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "pnpm run -C client dev",
    "beforeBuildCommand": "pnpm run -C client build"
  },
  "app": {
    "windows": [
      {
        "title": "ai-tauri-react-shadcn",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}